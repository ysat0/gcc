LIB1ASMSRC = i386/cygwin.asm
LIB1ASMFUNCS = _chkstk

# cygwin and mingw always have a limits.h, but, depending upon how we are
# doing the build, it may not be installed yet.
LIMITS_H_TEST = true

# If we are building next to winsup, this will let us find the real
# limits.h when building libgcc2.  Otherwise, winsup must be installed
# first.
LIBGCC2_INCLUDES = -I$(srcdir)/../winsup/w32api/include

winnt.o: $(srcdir)/config/i386/winnt.c
	$(COMPILE) $<

winnt-cxx.o: $(srcdir)/config/i386/winnt-cxx.c
	$(COMPILE) $<

winnt-stubs.o: $(srcdir)/config/i386/winnt-stubs.c
	$(COMPILE) $<

msformat-c.o: $(srcdir)/config/i386/msformat-c.c
	$(COMPILE) $<

STMP_FIXINC=stmp-fixinc
